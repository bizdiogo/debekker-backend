  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "dotenv-cli": "1.4.0",
    "graphql-cli": "2.16.5",
    "nodemon": "1.18.3",
    "npm-run-all": "4.1.3",
